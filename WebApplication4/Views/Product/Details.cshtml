@model WebApplication4.Models.TDanhMucSp

@{
    ViewData["Title"] = "Product Details";
    Layout = "~/Views/Product/_Layout1.cshtml";
}

<h1>Product Details</h1>

<div>
    <h4>@Model.TenSp</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.TenSp</dd>

        <dt class="col-sm-2">Laptop Compartment</dt>
        <dd class="col-sm-10">@Model.NganLapTop</dd>

        <dt class="col-sm-2">Model</dt>
        <dd class="col-sm-10">@Model.Model</dd>

        <dt class="col-sm-2">Weight</dt>
        <dd class="col-sm-10">@Model.CanNang</dd>

        <dt class="col-sm-2">Durability</dt>
        <dd class="col-sm-10">@Model.DoNoi</dd>

        <dt class="col-sm-2">Special Features</dt>
        <dd class="col-sm-10">@Model.MaDacTinh</dd>

        <dt class="col-sm-2">Website</dt>
        <dd class="col-sm-10">@Model.Website</dd>

        <dt class="col-sm-2">Warranty Period</dt>
        <dd class="col-sm-10">@Model.ThoiGianBaoHanh</dd>

        <dt class="col-sm-2">Introduction</dt>
        <dd class="col-sm-10">@Model.GioiThieuSp</dd>

        <dt class="col-sm-2">Discount</dt>
        <dd class="col-sm-10">@Model.ChietKhau</dd>

        <dt class="col-sm-2">Thumbnail</dt>
        <dd class="col-sm-10">@Model.AnhDaiDien</dd>

        <dt class="col-sm-2">Lowest Price</dt>
        <dd class="col-sm-10">@Model.GiaNhoNhat</dd>

        <dt class="col-sm-2">Highest Price</dt>
        <dd class="col-sm-10">@Model.GiaLonNhat</dd>

        <dt class="col-sm-2">Material</dt>
        <dd class="col-sm-10">@Model.MaChatLieuNavigation?.MaChatLieu</dd>

        <dt class="col-sm-2">Feature</dt>
        <dd class="col-sm-10">@Model.MaDtNavigation?.MaDt</dd>

        <dt class="col-sm-2">Manufacturer</dt>
        <dd class="col-sm-10">@Model.MaHangSxNavigation?.MaHangSx</dd>

        <dt class="col-sm-2">Category</dt>
        <dd class="col-sm-10">@Model.MaLoaiNavigation?.MaLoai</dd>

        <dt class="col-sm-2">Country of Origin</dt>
        <dd class="col-sm-10">@Model.MaNuocSxNavigation?.MaNuoc</dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.MaSp">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
